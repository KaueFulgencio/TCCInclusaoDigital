{
  "expo": {
    "name": "TCCInclusaoDigital",
    "slug": "TCCInclusaoDigital",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kauefulgencio.TCCInclusaoDigital",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"],
      "googleServicesFile": "./google-services.json",
      "package": "com.kauefulgencio.TCCInclusaoDigital"
    },
    "plugins": ["expo-font", "expo-asset"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d2b57a16-5720-4f41-8638-7d82a604903f"
      }
    }
  }
}
